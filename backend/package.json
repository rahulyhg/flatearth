{
  "name": "node-js",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production yarn dev",
    "dev": "rimraf ./dist && babel -d ./dist ./src -s && nodemon --delay 500ms  dev.js",
    "debug": "rimraf ./dist && babel -d ./dist ./src -s && nodemon --inspect=3001 dev.js",
    "test": "rimraf ./dist && ava -v -s",
    "test:coverage": "nyc report ava -- -v -s",
    "test:report": "nyc report ava -- -v -s --report=html",
    "test:watch": "yarn test -- --watch",
    "lint:js": "eslint ./src",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async-busboy": "*",
    "babel-cli": "^6.23.0",
    "babel-core": "next",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.1",
    "babel-plugin-dynamic-import-node": "^1.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "next",
    "babel-preset-es2015-node4": "^2.1.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.0",
    "config": "^1.25.1",
    "cross-env": "^5.0.0",
    "dotenv": "^4.0.0",
    "file-type": "^4.1.0",
    "flow-bin": "^0.49.1",
    "flow-io": "^0.1.0",
    "jsonwebtoken": "^7.3.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-favicon": "next",
    "koa-jwt": "^3.2.1",
    "koa-logger": "^3.0.1",
    "koa-mount": "^3.0.0",
    "koa-route": "^3.2.0",
    "lint-staged": "^3.3.1",
    "lodash": "^4.17.4",
    "log": "^1.4.0",
    "mime": "^1.3.4",
    "mongoose": "^4.9.5",
    "mysql": "^2.13.0",
    "mz": "^2.6.0",
    "rimraf": "^2.6.1",
    "sockjs": "^0.3.18",
    "tedious": "^2.0.0",
    "thunkify": "^2.1.2",
    "whatwg-fetch": "^2.0.3",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-register": "^6.23.0",
    "eslint": "4.1.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.0.1",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "pre-commit": "^1.2.2",
    "proxyquire": "^1.7.11",
    "sinon": "^2.1.0",
    "supertest": "^3.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "root": true,
    "plugins": [
      "flowtype",
      "react"
    ],
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "env": {
      "node": true
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint"
  },
  "pre-commit": "lint-staged",
  "ava": {
    "modules": true,
    "tap": "true",
    "require": [
      "babel-register"
    ],
    "failFast": true,
    "concurrency": 1,
    "failWithoutAssertions": false,
    "babel": "inherit",
    "match": [
      "!dist",
      "!.history",
      "**/*.test.js"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "flow"
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  }
}
